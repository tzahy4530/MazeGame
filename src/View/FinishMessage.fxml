<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane fx:id="congPane" prefHeight="400.0" prefWidth="400" stylesheets="@BGAndB.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.FinishMessage">
   <children>
      <Label fx:id="finishTitle" layoutX="30.0" layoutY="34.0" text="Congratulations" />
      <Button layoutX="159.0" layoutY="257.0" mnemonicParsing="false" onAction="#playAgain" onMouseClicked="#exit" text="Play Again" />
      <Button layoutX="177.0" layoutY="320.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Label fx:id="finishText1" layoutX="115.0" layoutY="123.0" text="You have reached the Goal!" />
      <Label fx:id="finishText2" layoutX="116.0" layoutY="161.0" text="Maze:" />
      <Label fx:id="finishText3" layoutX="116.0" layoutY="191.0" text="Total Steps:" />
      <Label fx:id="steps" layoutX="207.0" layoutY="192.0" textFill="RED">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label fx:id="mazeSize" layoutX="169.0" layoutY="162.0" textFill="RED">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
   </children>
</Pane>
