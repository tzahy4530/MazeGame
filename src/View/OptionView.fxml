<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<?import View.MazeDisplayer?>
<AnchorPane onMouseClicked="#mouseClicked" prefHeight="600.0" prefWidth="710.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MazeScene">
    <MazeDisplayer fx:id="mazeDisplayer" height="400.0" layoutX="8.0" layoutY="81.0" onKeyPressed="#keyPressed"
                   width="703.0"/>
    <Button fx:id="BackToMainScene" layoutX="8.0" layoutY="25.0" mnemonicParsing="false" onAction="#BackToMainScene"
            text="Back to Main"/>
    <Button fx:id="SolveButton" layoutX="662.0" layoutY="25.0" mnemonicParsing="false" onAction="#SolveMaze"
            text="Solve"/>
</AnchorPane>
