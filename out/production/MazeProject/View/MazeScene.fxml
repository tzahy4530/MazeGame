<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<?import View.MazeDisplayer?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Pane?>
<VBox onMouseClicked="#mouseClicked" prefHeight="600.0" prefWidth="710.0" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MazeScene">
<children>
    <Pane fx:id="pane" VBox.vgrow="ALWAYS">
        <children>
            <MazeDisplayer fx:id="mazeDisplayer" height="${pane.height}" width="${pane.width}" onKeyPressed="#keyPressed"/>
        </children>
    </Pane>
</children>
</VBox>